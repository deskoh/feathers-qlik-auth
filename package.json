{
  "name": "feathers-qlik-auth",
  "description": "FeathersJS QlikSense AWS Cognito Authentication",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "deskoh"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^10.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run compile && npm run mocha",
    "dev": "ts-node-dev --no-notify src/",
    "start": "npm run compile && node lib/",
    "start:docker": "docker run --rm -it -p 3030:3030 --env-file ./.env qlik-auth",
    "build:docker": "docker build -t qlik-auth .",
    "eslint": "eslint --ext ts,js .",
    "mocha": "ts-mocha \"test/**/*.ts\" --recursive --exit",
    "compile": "shx rm -rf lib/ && tsc"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
    "@feathersjs/authentication": "^4.5.3",
    "@feathersjs/authentication-local": "^4.5.3",
    "@feathersjs/authentication-oauth": "^4.5.3",
    "@feathersjs/configuration": "^4.5.3",
    "@feathersjs/errors": "^4.5.3",
    "@feathersjs/express": "^4.5.3",
    "@feathersjs/feathers": "^4.5.3",
    "@feathersjs/transport-commons": "^4.5.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.2.0",
    "express-session": "^1.17.1",
    "helmet": "^3.22.0",
    "jose": "^1.28.2",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.3",
    "jws": "^4.0.0",
    "memorystore": "^1.6.2",
    "ms": "^2.1.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express-session": "^1.17.0",
    "@types/helmet": "0.0.46",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/jws": "^3.2.2",
    "@types/mocha": "^7.0.2",
    "@types/ms": "^0.7.31",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "axios": "^0.21.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-plugin-import": "^2.20.1",
    "mocha": "^7.1.2",
    "nodemon": "^2.0.3",
    "shx": "^0.3.2",
    "ts-mocha": "^7.0.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  }
}
