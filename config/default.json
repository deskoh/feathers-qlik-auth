{
  "host": "localhost",
  "port": 3030,
  "authentication": {
    "defaults": {
      "prefix": "/qlik",
      "transport": "session"
    },
    "cognito": {
      "callback": "/qlik/callback",
      "oauth": 2,
      "scope": ["openid"],
      "scope_delimiter": " ",
      "subdomain": "{domain}.{region}.amazoncognito.com",
      "key": "<cognito client id>",
      "secret": "<cognito client secret>",
      "userPoolId": "<userPoolId>",
      "region": "ap-southeast-1",
      "nonce": true
    }
  },
  "qlik": {
    "proxyRestUri": "http://localhost:3030/mockQlik",
    "userDirectory": "SomeDomain",
    "rootCa": "../certs/ca.pem",
    "clientKey": "../certs/client_key.pem",
    "clientCert": "../certs/client_cert.pem"
  },
  "mockCognito": {
    "path": "/mockCognito",
    "atPayload": {
      "cognito:groups": [
        "MyGroup"
      ],
      "username": "anonoymous",
      "version": 2
    },
    "idPayload": {
      "sub": "75b327dc-c42c-41c9-8c3f-8b2f462c617a",
      "cognito:groups": [
        "MyGroup"
      ],
      "name": "Anonoymous",
      "cognito:username": "anonoymous"
    },
    "userInfo": {
      "sub": "75b327dc-c42c-41c9-8c3f-8b2f462c617a",
      "name": "Anonymous",
      "username": "anonoymous"
    }
  }
}
